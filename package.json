    "object-to-exports": "esrun ops/transformers/object-to-exports lib/motion/config/constants/generate.ts",
    "obfuscate": "ops/screens/obfuscate.sh",
    "ops:outreacher:backend": "esrun ops/outreacher/index.ts",
    "outreacher": "npm run dev & npm run ops:outreacher:backend"
  },
